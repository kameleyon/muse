{
    "name": "magicmuse",
    "private": true,
    "version": "0.1.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "dev:server": "cd server && npm run dev",
        "dev:all": "concurrently \"npm run dev\" \"npm run dev:server\"",
        "build": "npx tsc && npx vite build",
        "build:server": "cd server && npm run build",
        "build:all": "npm install && npm run build && npm run build:server",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "test": "jest",
        "start": "vite preview --host --port $PORT",
        "prepare": "node -e \"if (!process.env.CI && !process.env.HUSKY) { try { require('husky').install() } catch (e) { console.warn('Husky installation skipped') } }\"",
        "postinstall": "node -e \"if (process.platform !== 'win32') { require('child_process').execSync('chmod +x ./node_modules/.bin/*') }\""
    },
